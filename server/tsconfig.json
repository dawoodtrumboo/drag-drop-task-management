{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "module": "commonjs",
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "target": "es6",
    "outDir": "./dist",
    "skipLibCheck": true,
   },
   "include": ["src/**/*.ts"],
   "exclude": ["node_modules"]
   
}